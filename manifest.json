
{
   "name": "RequestWatcher",

   "browser_action": {
     "default_title": "RequestWatcher",
     "default_popup": "popup.html",
     "default_icon": {
       "16": "icon16.png"
     }
   },
   "version": "1.0",
   "manifest_version": 2,
   "background": {
       "scripts": ["libraries/jquery.min.js", "logip.js"]
   },

   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

   "permissions": [
       "webRequest",
       "<all_urls>",
       "tabs",
       "proxy"
   ]
}
